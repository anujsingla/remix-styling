{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "sass-dev": "sass --watch --update --style=expanded app/styles/sass:app/styles/css",
    "sass-prod": "sass --no-source-map --style=compressed app/styles/sass:app/styles/css",
    "dev:css": "concurrently \"npm run sass-dev\" \"remix dev\""
  },
  "dependencies": {
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "@types/axios": "^0.14.0",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "concurrently": "^7.0.0",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.0",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1",
    "sass": "^1.47.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}